assign(max_models,1).
set(arithmetic).

formulas(square_family).
  a1 < a2 & a2 < a3 & a3 < a4 & a4 < a5 & a5 < a6 & a6 < a7 & 
  a7 < a8 & a8 < a9 & a9 < father.
  
  a1 * a1 + a2 * a2 + a3 * a3 + a4 * a4 + a5 * a5 + 
  a6 * a6 + a7 * a7 + a8 * a8 + a9 * a9 = father * father.

  a2 + -a1 = a3 + -a2.
  a3 + -a2 = a4 + -a3.
  a4 + -a3 = a5 + -a4.
  a5 + -a4 = a6 + -a5.
  a6 + -a5 = a7 + -a6.
  a7 + -a6 = a8 + -a7.
  a8 + -a7 = a9 + -a8.
end_of_list.


